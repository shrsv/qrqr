{
  "manifest_version": 2,

  "name": "qrqr",
  "description": "Create QR code for current page URL",
  "version": "1.0",
  "icons": {
      "48": "icon.png"
  },

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Generate QR code!"
  },
  "permissions": [
    "activeTab"
  ]
}
